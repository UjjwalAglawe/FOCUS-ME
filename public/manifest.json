{
    "name": "DissTract",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "tabs",
      "storage",
      "webNavigation"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "web_accessible_resources": [
      {
        "resources": ["focus.html", "res/*"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  